<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	
	<!-- HibernateUtil -->
	<bean id="hibernateutil" class="master.realist.REAlistGUIGenerator.server.util.HibernateUtil"></bean>
	
	<!-- DAOs -->
    <bean id="dualitytypedao" class="master.realist.REAlistGUIGenerator.server.daos.DualitytypeDAO">
    	<property name="hibernateUtil" ref="hibernateutil"></property>
    </bean>
    
    <bean id="dualitystatusdao" class="master.realist.REAlistGUIGenerator.server.daos.DualityStatusDAO">
    	<property name="hibernateUtil" ref="hibernateutil"></property>
    </bean>
    
    <bean id="agentdao" class="master.realist.REAlistGUIGenerator.server.daos.AgentDAO">
    	<property name="hibernateUtil" ref="hibernateutil"></property>
    </bean>
    
    <bean id="agenttypedao" class="master.realist.REAlistGUIGenerator.server.daos.AgenttypeDAO">
    	<property name="hibernateUtil" ref="hibernateutil"></property>
    </bean>
    
    <bean id="dualitydao" class="master.realist.REAlistGUIGenerator.server.daos.DualityDAO">
    	<property name="hibernateUtil" ref="hibernateutil"></property>
    </bean>
    
    <bean id="eventdao" class="master.realist.REAlistGUIGenerator.server.daos.EventDAO">
    	<property name="hibernateUtil" ref="hibernateutil"></property>
    </bean>
    
    <bean id="eventHasAdditionalattributevalueDAO" class="master.realist.REAlistGUIGenerator.server.daos.EventHasAdditionalattributevalueDAO">
    	<property name="hibernateUtil" ref="hibernateutil"></property>
    </bean>
	
</beans>